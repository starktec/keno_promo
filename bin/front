server {
        server_name f12bet.mrkeno.com;
        client_max_body_size 4G;
        location / {
                try_files $uri /index.html;
                alias   /home/project/app/;
        }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/f12bet.mrkeno.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/f12bet.mrkeno.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = f12bet.mrkeno.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


listen 80;
        server_name f12bet.mrkeno.com;
    return 404; # managed by Certbot


}